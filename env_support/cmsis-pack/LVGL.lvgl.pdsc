    <condition id="LVGL-CORE">
      <description>Require LVGL Core</description>
      <require Cclass="LVGL" Cgroup="CORE"/>
    </condition>

    <condition id="Arm-2D">
      <description>Require Arm-2D Support</description>
      <require Cclass="LVGL" Cgroup="Arm-2D"/>
    </condition>

    <condition id="STM32-DMA2D HAL">
        <description>STM32-DMA2D HAL Dependencies</description>
        <require condition="LVGL-CORE"/>
        <require Dvendor="STMicroelectronics:13"/>
        <deny Cbundle="STMicro_Driver" Cclass="Device" Cgroup="DMA2D" Csub="HAL"/> <!-- STM32CubeMX: DMA2D HAL -->
        <require Cclass="Device" Cgroup="STM32Cube HAL" Csub="DMA2D"/> <!-- Keil: DMA2D HAL -->
    </condition>

    <condition id="STM32-DMA2D HAL - STM32CubeMX">
        <description>STM32-DMA2D HAL Dependencies as defined in STM32CubeMX</description>
        <require condition="LVGL-CORE"/>
        <require Dvendor="STMicroelectronics:13"/>
        <deny Cclass="Device" Cgroup="STM32Cube HAL" Csub="DMA2D"/> <!-- Keil: DMA2D HAL -->
        <!-- <require Cbundle="STMicro_Driver" Cclass="Device" Cgroup="DMA2D" Csub="HAL"/> --> <!-- STM32CubeMX: DMA2D HAL (NOT YET IMPLEMENTED) -->
    </condition>

    <condition id="Arm-2D HAL">
        <description>Arm-2D HAL Dependencies</description>
        <require condition="LVGL-CORE"/>
        <deny Dvendor="STMicroelectronics:13"/>
    </condition>

    <condition id="SWM341-DMA2D HAL">
        <description>SWM341-DMA2D HAL Dependencies</description>
        <require condition="LVGL-CORE"/>
        <deny Dvendor="STMicroelectronics:13"/>
    </condition>

    <condition id="NXP-PXP HAL">
        <description>NXP-PXP HAL Dependencies</description>
        <require condition="LVGL-CORE"/>
        <deny Dvendor="STMicroelectronics:13"/>
    </condition>

    <condition id="NXP-VGLite HAL">
        <description>NXP-VGLite HAL Dependencies</description>
        <require condition="LVGL-CORE"/>
        <deny Dvendor="STMicroelectronics:13"/>
    </condition>

    <condition id="GD32-IPA HAL">
        <description>GD32-IPA HAL Dependencies</description>
        <require condition="LVGL-CORE"/>
        <deny Dvendor="STMicroelectronics:13"/>
    </condition>

    <condition id="LVGL-Extra Widgets">
      <description>Require Extra Widgets</description>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Animing"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Arc"/>

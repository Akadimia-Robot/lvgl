      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Dropdown"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Image"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Image Button"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Keyboard"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Label"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="LED"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Line"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="List"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Menu"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Meter"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Message Box"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Roller"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Slider"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Span"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Spin Box"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Spinner"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Switch"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Table"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Tab View"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Text Area"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Title View"/>
      <require Cclass="LVGL" Cgroup="Extra Widgets" Csub="Window"/>
    </condition>

    <condition id="LVGL-Extras">
      <description>Require Extras</description>
      <require Cclass="LVGL" Cgroup="Themes" Csub="Basic"/>
      <require Cclass="LVGL" Cgroup="Layouts" Csub="Flex"/>
      <require Cclass="LVGL" Cgroup="Layouts" Csub="Grid"/>
      <require condition="LVGL-Extra Widgets"/>
    </condition>

    <condition id="LVGL-ThemeDefault">
      <description>Require Theme Default</description>
      <accept Cclass="LVGL" Cgroup="Themes" Csub="Default Dark"/>
      <accept Cclass="LVGL" Cgroup="Themes" Csub="Default Light"/>
    </condition>
  </conditions>

